<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eyeclinic.Pages.App.PatientsList.Controller">
   <children>
      <HBox>
         <children>
            <Label text="Patients">
               <font>
                  <Font size="28.0" />
               </font>
            </Label>
            <Region HBox.hgrow="ALWAYS" />
            <Button fx:id="createNewPatientButtonHandler" mnemonicParsing="false" onAction="#createPatientButtonHandler" text="Create new" />
         </children>
      </HBox>
      <VBox fx:id="container" spacing="5.0" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
