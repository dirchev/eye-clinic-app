<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root styleClass="card" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
         <HBox>
          <children>
             <Label fx:id="patientNameLabel" styleClass="card-title" text="Dimitar Mirchev">
                <font>
                   <Font size="22.0" />
                </font>
             </Label>
             <Region HBox.hgrow="ALWAYS" />
             <Button fx:id="viewPatientButton" mnemonicParsing="false" onAction="#previewPatient" styleClass="card-button" text="View" />
          </children>
          <padding>
             <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
          </padding>
       </HBox>
   </children>
</fx:root>
